{
    "name": "explorema",
    "version": "1.0.0",
    "scripts": {
      "start": "npm run start:prod",
      "start:frontend": "serve -s build",
      "start:backend": "nodemon backend/server.js",
      "build": "react-scripts build --output-path build",
      "vercel-build": "npm run build",
      "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
      "dev:frontend": "react-scripts start",
      "dev:backend": "nodemon backend/server.js",
      "start:prod": "npm run build:backend && npm run start:frontend & npm run start:backend",
      "build:backend": "echo \"No build step for backend\""
    },
    "dependencies": {
      "axios": "^1.3.4",
      "concurrently": "^7.0.0",
      "cors": "^2.8.5",
      "dotenv": "^16.4.5",
      "express": "^4.19.2",
      "leaflet": "^1.7.1",
      "mysql2": "^3.10.3",
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "react-leaflet": "^4.2.0",
      "react-modal": "^3.14.0",
      "react-scripts": "5.0.1",
      "serve": "^14.0.1",
      "web-vitals": "^2.1.4"
    },
    "devDependencies": {
      "nodemon": "^3.1.4"
    }
  }
  